
version = 2

[general]
name = Shader.Builtin.UI2D
depthTest = false
depthWrite = false
stencilTest = true
stencilWrite = true
maxInstances = 512
[/general]

[stages]
vertex = Builtin.UI2DShader.vert
fragment = Builtin.UI2DShader.frag
[/stages]

[attributes]
inPosition = vec2
inTexCoord = vec2
[/attributes]

[uniforms]
[global]
projection = mat4
view = mat4
[/global]
[instance]
properties = struct16
diffuseTexture = sampler2D
[/instance]
[local]
model = mat4
[/local]
[/uniforms]
