
version = 1
name = Shader.Builtin.UI2D
stages = vertex, fragment
stageFiles = shaders/Builtin.UI2DShader.vert, shaders/Builtin.UI2DShader.frag
depthTest = false
depthWrite = false
stencilTest = true
